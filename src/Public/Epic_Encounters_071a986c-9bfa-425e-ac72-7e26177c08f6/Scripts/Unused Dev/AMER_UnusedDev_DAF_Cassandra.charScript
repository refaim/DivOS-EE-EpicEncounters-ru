	StopLoopEffect(%ReusableHandle_01)
	StopLoopEffect(%ReusableHandle_02)
	StopLoopEffect(%ReusableHandle_03)
	StopLoopEffect(%ReusableHandle_04)
	StopLoopEffect(%ReusableHandle_05)